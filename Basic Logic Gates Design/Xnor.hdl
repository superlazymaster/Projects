/**
 * Exclusive-Nor gate:
 * out = (a == b)
 */

CHIP Xnor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=a, b=b, out=w1);
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    And(a=notA, b=notB, out=w2);
    Or(a=w1, b=w2, out=out);
}