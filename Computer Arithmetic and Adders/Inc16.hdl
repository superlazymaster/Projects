/**
 * 16-bit incrementer:
 * out = in + 1 (arithmetic addition)
 * in is UNSIGNED
 * carry output is intentionally ignored
 */

CHIP Inc16 {
    IN in[16];
    OUT out[16];

    PARTS:
    Add16(a=in, b[0]=true, b[1..15]=false, out=out);
}